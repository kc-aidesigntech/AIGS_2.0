          "_name": "tabs01",
          "_sourceTheme": "shapem5",
          "_customHTML": "<section class=\"tabs01 tabs shapem5\" group=\"List\" plugins=\"mbr-tabs\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Label\" name=\"showLabel\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#67a1e5\" name=\"label\" condition=\"showLabel\" selected>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <header>Tabs</header>\n        <select title=\"Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"color\" title=\"Tabs Color\" value=\"#67a1e5\" name=\"tabs\" selected>\n        <input type=\"color\" title=\"Border Color\" value=\"#e0e0e0\" name=\"border\" selected>\n        <input type=\"color\" title=\"Tabs Active Color\" value=\"#ffffff\" name=\"activeColor\" selected>\n        <header>Cards</header>\n        <input type=\"checkbox\" title=\"Image Decoration\" name=\"showDecor\" checked condition=\"bg.type === 'color'\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Card Buttons\" name=\"showButtons\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/img-0658.jpg-1440x1080.jpg\" selected parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/shapem5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-5\">\n                <div class=\"title-wrapper\">\n                    <div class=\"label-wrapper\">\n                        <p class=\"mbr-label mbr-fonts-style\" data-app-selector=\".mbr-label, .label-wrapper\" mbr-theme-style=\"display-4\" mbr-if=\"showLabel\"><b>Ready for you in a pinch</b></p>\n                    </div>\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .tabs-wrapper\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                        <b>Hardware &amp; Supplies</b></h2>\n                </div>\n                <div class=\"tabs-wrapper\">\n                    <ul class=\"nav nav-tabs\" role=\"tablist\">\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item first mbr-fonts-style\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link show mbr-fonts-style active\" data-app-placeholder=\"Type Text\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#hardware_heading_tab0\" aria-selected=\"true\">Automative &amp; Electrical</a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 1\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" data-app-placeholder=\"Type Text\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#hardware_heading_tab1\" aria-selected=\"true\">Plumbing</a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 2\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link show mbr-fonts-style active\" data-app-placeholder=\"Type Text\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#hardware_heading_tab2\" aria-selected=\"true\">Gardening</a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-5\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 3\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link show mbr-fonts-style active\" data-app-placeholder=\"Type Text\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#hardware_heading_tab3\" aria-selected=\"true\">Basic work supplies</a></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"tab-content\">\n                    <div id=\"tab1\" class=\"tab-pane in active\" role=\"tab